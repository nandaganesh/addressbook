pipeline {
    agent none
    stages {
        stage('COMPILE') {
            agent any
            steps {
                script {
                    echo "compile the code"
                }
            }
        }
        stage('TEST') {
            agent any
            steps {
                script {
                    echo "compile the code"
                }
            }
        }
        stage('PACKAGE') {
            agent any
            steps {
                script {
                 
                    echo "compile the code"
                   
                    sh "mvn package"
                    
                }
            }
        } 
        
    }
}